    width: 100%;
    height: 1rem;
    .border_1px(top);
    position: fixed;
    bottom: 0;
    background: #fff;
    overflow: hidden;
    button{
        width: 2.4rem;
        background: @main_color;
        font-size: 0.32rem;
        color: #fff;
        display: block;
        float: right;
        height: 1rem;
        margin-top: 0.01rem;
    }
    p{
        line-height: 0.99rem;
        font-size: 0.32rem;
        color: @main_color;
        text-align: right;
        padding-right: 0.35rem;
        display: inline-block;
        width: 4rem;
        float: right;
        span{
          font-size: 0.32rem;
        }
    }
}

/*评价--value*/
.value_wrap{
    .value_item{
        line-height: 0.4rem;
        padding: 0.29rem 0.3rem;
        background: #fff;
    }
    .total_value{
        width: 100%;
        background: #fff;
        margin-top: 0.2rem;
        line-height: 1rem;
        label{
            font-size:0.28rem;
            color: @font_color;
            margin-left: 0.36rem;
            line-height: 1rem;
            display: inline-block;
            &.on{
                color: @main_color;
            }
            &:nth-of-type(1){
                background:url(../img/icon_good.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            &:nth-of-type(2){
                background:url(../img/icon_soso.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            &:nth-of-type(3){
                background:url(../img/icon_poor.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            &:nth-of-type(1).on{
                background:url(../img/icon_good2.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            &:nth-of-type(2).on{
                background:url(../img/icon_soso2.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            &:nth-of-type(3).on{
                background:url(../img/icon_poor2.png) no-repeat center left;
                background-size: 0.28rem 0.28rem;
            };
            input{
                display: inline-block;
                width: 0.28rem;
                height: 0.28rem;
                visibility: hidden;
                margin-right: 0.16rem;
            }
        }
    }
    .value_con{
        width: 100%;
        background: #fff;
        .border_1px(bottom);
        .border_1px(top);
        textarea{
            resize: none;
            display: block;
            height: 1.56rem;
            padding: 0.24rem 0;
            font-size: 0.24rem;
        }
    }
    .num_value{
        line-height: 0.66rem;
        font-size: 0.24rem;
        color: @font_color3;
        text-align: right;
        letter-spacing: 1px;
    }
    .serve_value{
        width: 100%;
        background: #fff;
        padding-top: 0.3rem;
        margin-top: 0.2rem;
        h2{
            margin-bottom: 0.35rem;
        }
        .sort_serve{
            margin-bottom: 0.25rem;
            p{
                font-size: 0.28rem;
                color: @font_color2;
                display: inline-block;
                vertical-align: top;
                line-height: 0.32rem;
            }
            ul{
                display: inline-block;
                margin-left: 0.3rem;
                li{
                    width: 0.33rem;
                    height: 0.32rem;
                    background: url(../img/icon_star2.png) no-repeat;
                    background-size: 100%;
                    display: inline-block;
                    margin:0 0.07rem;
                    &.on{
                        background: url(../img/icon_star3.png) no-repeat;
                        background-size: 100%;
                    }
                }
            }
            span{
                font-size: 0.26rem;
                color: @font_color3;
                float: right;
                line-height: 0.32rem;
            }
        }
    }
    .faceless{
        .border_1px(top);
        line-height: 1rem;
        span{
            float: right;
            font-size: 0.24rem;
            color: @font_color3;
        }
        p{
            font-size: 0.24rem;
            color: @font_color3;
            display: inline-block;
            width: 2rem;
            &.on{
                color: @main_color;
            }
            input{
                width: 0.28rem;
                height: 0.98rem;
                background: url(../img/icon_faceless.png) no-repeat left center;
                background-size: 0.28rem 0.28rem;
                margin-right: 0.17rem;
                vertical-align: top;
            }
            &.on>input{
                background: url(../img/icon_faceless2.png) no-repeat left center;
                background-size: 100%;
            }
        }
    }
}

/*优惠券--coupon*/
.coupon_wrap{
    .coupon_tit{
        width: 100%;
        height: 0.9rem;
        background: #fff;
        font-size: 0;
        .border_1px(bottom);
        a{
            display: inline-block;
            width: 50%;
            line-height: 0.9rem;
            color: @font_color2;
            text-align: center;
            font-size: 0.26rem;
            position: relative;
            &.on{
                color: @main_color;
            }
            &.on:after{
                content:"";
                display: block;
                width: 1.08rem;
                height: 1px;
                background: @main_color;
                bottom: 0;
                left: 0;
                right: 0;
                margin: auto;
            };
            &:nth-of-type(1):before{
                content:"";
                display: block;
                width: 1px;
                height: 0.56rem;
                right: 0;
                top: 0.17rem;
                position: absolute;
                background: #e8e8e8;
            };
        }
    }
    .coupon_code{
        height: 0.76rem;
        line-height: 0.76rem;
        background: #fff;
        overflow: hidden;
        border-radius: 0.1rem;
        margin-top: 0.2rem;
        form{
            width: 100%;
            input{
                width: 72%;
                vertical-align: top;
                line-height: 0.76rem;
                font-size: 0.26rem;
                text-indent: 0.25rem;
            }
            button{
                width: 25%;
                display: inline-block;
                vertical-align: top;
                background: #fff;
                font-size: 0.28rem;
                color: @main_color;
                line-height: 0.46rem;
                float: right;
                border-left: @main_color solid 1px;
                margin-top: 0.15rem;
            }
        }
    }
    .coupon_list{
        margin-bottom: 0.5rem;
        &>ul>li{
            width: 6.68rem;
            background: url(../img/icon_yhj.png) no-repeat center;
            background-size: 100% 100%;
            min-height: 2.3rem;
            margin-top: 0.2rem;
            position: relative;
            color: @font_color3;
            &.on{
                background: url(../img/coupon.png) no-repeat center;
                background-size: 100% 100%;
            }
            &.expire:after{
                content:"";
                display: block;
                position: absolute;
                width: 1.24rem;
                height: 0.95rem;
                background: url(../img/icon_expire2.png) no-repeat;
                background-size: 100%;
                top: 0.045rem;
                left: 2.1rem;
                z-index: 99;
            }
            &.freeze:after{
                content:"";
                display: block;
                position: absolute;
                width: 1.24rem;
                height: 0.95rem;
                background: url(../img/icon_freeze.png) no-repeat;
                background-size: 100%;
                top: 0.045rem;
                left: 2.1rem;
                z-index: 99;
            }
            &.abate:after{
                content:"";
                display: block;
                position: absolute;
                width: 1.24rem;
                height: 0.95rem;
                background: url(../img/icon_expire.png) no-repeat;
                background-size: 100%;
                top: 0.045rem;
                left: 2.1rem;
                z-index: 99;
            }
            &>h1{
                width: 6rem;
                margin:0 auto;
                font-size: 0.24rem;
                line-height: 0.66rem;
                padding-top: 0.02rem;
                background: url(../img/line.png) no-repeat bottom center;
                background-size: 100%;
                span{
                    font-size: 0.22rem;
                    float: right;
                }
            }
            &>ol{
                display: inline-block;
                vertical-align: top;
                margin-top: 0.25rem;
                vertical-align: top;
                padding-left: 0.3rem;
                margin-bottom: 0.2rem;
                width: 4.4rem;
                li{
                    font-size: 0.22rem;
                    color: @font_color3;
                    margin-bottom: 0.075rem;
                    b{
                        font-size: 0.26rem;
                    }
                }
            }
            .coupon_price{
                display: inline-block;
                vertical-align: top;
                float: right;
                padding-right: 0.3rem;
                margin-top: 0.2rem;
                h1{
                    font-size: 0.73rem;
                    line-height: normal;
                    span{
                        font-size: 0.3rem;
                    }
                }
                p{
                    font-size: 0.2rem;
                    color: @font_color3;
                    b{
                        width: 0.2rem;
                        height: 0.11rem;
                        background: url(../img/icon_arrow2.png) no-repeat center;
                        background-size: 100%;
                        display: inline-block;
                        margin-left: 0.06rem;
                        transition: all 0.4s linear;
                        -ms-transition: all 0.4s linear;
                        -moz-transition: all 0.4s linear;
                        -webkit-transition: all 0.4s linear;
                        -o-transition: all 0.4s linear;
                        &.turn_up{
                            -ms-transform: rotate(180deg);
                            /* IE 9 */
                            -moz-transform: rotate(180deg);
                            /* Firefox */
                            -webkit-transform: rotate(180deg);
                            /* Safari 和 Chrome */
                            -o-transform: rotate(180deg);
                            transform: rotate(180deg);
                            /* Opera */
                            transition: all 0.4s linear;
                            -ms-transition: all 0.4s linear;
                            -moz-transition: all 0.4s linear;
                            -webkit-transition: all 0.4s linear;
                            -o-transition: all 0.4s linear;
                        }
                    }
                }
            }
            .coupon_detail{
                padding-top: 0.15rem;
                display: none;
                padding-bottom: 0.3rem;
                clear: both;
                width: 6rem;
                margin:0 auto;
                background: url(../img/line.png) no-repeat top center;
                background-size: 100%;
                h3,p{
                    font-size: 0.2rem;
                    color: @font_color3;
                    line-height: 0.35rem;
                }
            }
        }
    }
}

/*订单列表--order-list*/
.order_list_wrap{
    .order_sort{
        width: 100%;
        line-height: 0.9rem;
        overflow: hidden;
        background: #fff;
        .border_1px(bottom);
        li{
            width: 20%;
            text-align: center;
            font-size: 0.26rem;
            color: @font_color3;
            float: left;
            position: relative;
            &.on{
                color: @main_color;
            }
            &.on:after{
                content: "";
                display: block;
                position: absolute;
                width: 1.08rem;
                height: 1px;
                background: @main_color;
                bottom: 0;
                left: 0;
                right: 0;
                margin:auto;
                z-index: 99;
            };
        }
    }
    .order_list{
        width: 100%;
        margin-bottom: 0.78rem;
        ul{
            &.none{
                display: none;
            }
            li{
                margin-top: 0.2rem;
                background: #fff;
                .order_code{
                    font-size: 0.24rem;
                    .border_1px(bottom);
                    h1{
                        color: @font_color;
                        line-height: 0.84rem;
                        display: inline;
                    }
                    span{
                        float: right;
                        line-height: 0.84rem;
                    }
                }
                .order_information{
                    a{
                        display: block;
                        padding: 0.4rem 0;
                        img{
                            width: 1.56rem;
                            height: 1.56rem;
                            display: inline-block;
                            vertical-align: top;
                            margin-right: 0.3rem;
                        }
                        .order_information_txt{
                            width: 4.66rem;
                            display: inline-block;
                            vertical-align: top;
                            h1{
                                font-size: 0.28rem;
                            }
                            p{
                                font-size: 0.22rem;
                                color: @font_color2;
                                margin-top: 0.06rem;
                            }
                            span{
                                display: block;
                                font-size: 0.24rem;
                                color: @font_color;
                                background: url(../img/icon_addr4.png) no-repeat left;
                                background-size: 0.18rem 0.24rem;
                                margin-top: 0.05rem;
                                text-indent: 0.27rem;
                                background-position-y:0.08rem;
                                line-height: 0.4rem;
                            }

                        }
                    }
                }
                .order_btn_wrap{
                    .border_1px(top);
                    overflow: hidden;
                    a{
                        display: inline-block;
                        width: 1.4rem;
                        height: 0.56rem;
                        border-radius: 0.06rem;
                        border:@main_color solid 1px;
                        font-size: 0.24rem;
                        color: @main_color;
                        line-height: 0.56rem;
                        text-align: center;
                        margin:0.24rem 0;
                        float: right;
                        margin-left: 0.3rem;
                        &.cancel{
                            border-color:#9d9d9d;
                            color: #666;
                        }
                    }
                    span{
                        font-size: 0.24rem;
                        line-height: 0.45rem;
                        margin-top: .3rem;
                        display: inline-block;
                        color: @font_color3;
                        &.red_color{
                            color: #e8321b;
                            margin-top: 0.12rem;
                        }
                        b{
                            display: block;
                            line-height: 0.3rem;
                            color: @font_color3;
                        }

                    }
                }
            }
        }
    }
}

/*维修详情--选择服务弹框*/
.blank_wrapper{
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0,0,0,0.6);
    z-index: 100;
    opacity: 0;
    display: none;
}
.layer_details{
    position: fixed;
    display: none;
    bottom: -100%;
    z-index: 120;
    width: 100%;
    background: #fff;
    padding-top: 0.35rem;
    .close_btn{
        display: block;
        width: 0.37rem;
        height: 0.37rem;
        background: url(../img/icon_close1.png) no-repeat center;
        padding: 0.2rem;
        z-index: 100;
        background-size: 0.37rem 0.37rem;
        position: absolute;
        right: 0rem;
        top: 0rem;
    }
    .layer_details_head{
        .border_1px(bottom);
        padding-bottom: 0.35rem;
        img{
            width: 2.2rem;
            height: 2.2rem;
            display: inline-block;
            vertical-align: top;
            margin-top: -0.9rem;
        }
        .head_tit{
            width:4.1rem;
            display: inline-block;
            margin-left: 0.22rem;
            vertical-align: top;
            .limit{
                font-size: 0.36rem;
                color: @main_color;
                margin-top: 0.1rem;
                s{
                    color: @font_color3;
                    font-size: 0.26rem;
                    margin-left: 0.05rem;
                }
                span{
                    display: inline-block;
                    margin-left: 0.2rem;
                    width: 1.19rem;
                    height: 0.34rem;
                    background: url(../img/icon_limit.png) no-repeat;
                    background-size: 100%;
                }
            }
            .unlimit{
                color: @main_color;
                line-height: 0.45rem;
                font-size: 0.22rem;
                display: inline-block;
                vertical-align: top;
                span{
                    font-size: 0.36rem;
                }
            }
            .choose_item{
                font-size: 0.26rem;
                margin-top: 0.15rem;
                line-height: 0.4rem;
                b{
                    float: right;
                    line-height: 0.36rem;
                }
                span{
                    display: inline-block;
                    vertical-align: top;
                    width: 53%;
                    line-height: 0.36rem;
                }
            }
        }
    }
    .layer_details_body{
        width: 6.9rem;
        margin-left: 0.3rem;
        max-height: 6.1rem;
        overflow-y:scroll;
        .layer_serve{
            h1{
                margin: 0.3rem 0;
            }
            span{
                display: inline-block;
                line-height: 0.6rem;
                padding: 0 0.3rem;
                color: @font_color3;
                border:#b3b3b3 solid 1px;
                vertical-align: top;
                border-radius: 30px;
                margin-right: 0.2rem;
                margin-bottom: 0.3rem;
                &.on{
                    background: @main_color;
                    color: #fff;
                    border-color: @main_color;
                }
            }
        }
        &>p{
            .border_1px(top);
            width: 6.6rem;
            line-height: 0.88rem;
            span{
                font-size: 0.28rem;
                color: @main_color;
                float: right;
            }
        }
        &>h2{
            .border_1px(top);
            width: 6.6rem;
            line-height: 1.06rem;
            p{
                float: right;
                font-size: 0;
                a,span{
                    display: inline-block;
                    vertical-align: top;
                    width: 0.56rem;
                    height: 0.56rem;
                    font-size: 0.26rem;
                    line-height: 0.56rem;
                    text-align: center;
                    background: #e8e8e8;
                    margin: 0.25rem 0;
                }
                span{
                    width: 0.7rem;
                    background: #fff;
                    color: @font_color2;
                }
            }
        }
        .special_tip{
            .border_1px(top);
            h2{
                padding-top: 0.3rem;
            }
            p{
                color: @font_color2;
                font-size: 0.24rem;
                line-height: 0.44rem;
                margin-top: 0.12rem;
                padding-bottom: 0.45rem;
            }
        }
    }
    .buy_foot{
        position: relative;
    }
}

/*购物车--shoppingcart*/
.shop_car_list{
    margin-bottom: 1.2rem;
    li{
        width: 100%;
        background: #fff;
        position: relative;
        margin-bottom: 0.2rem;
        overflow: hidden;
        &>h1{
            padding: 0 0.3rem;
            line-height: 0.94rem;
            .border_1px(bottom);
            font-size: 0.3rem;
            span{
                font-size: 0.26rem;
                color: @main_color;
                float: right;
                &.complete_store{
                    display: none;
                }
            }
        }
        .wrap{
            display: block;
            font-size: 0;
            padding: 0.42rem 0;
            &>b{

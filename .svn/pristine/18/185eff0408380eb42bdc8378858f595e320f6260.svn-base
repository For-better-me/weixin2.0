}
.serve_hot{
    background: #fff;
    margin-bottom: 1.2rem;
    .dropload-down{
        .border_1px(top);
        height: 0.8rem;
        line-height: 0.8rem;
        text-align: center;
        span{
            font-size: 0.26rem;
            color: #cdcdcd;
        }
    }
}
.serve_hot{
    .item_list{
        li{
            .border_1px(top);
        }
    }
}

/*注册 --register*/
.register{
    .agreement{
        margin-top: 0.25rem;
        margin-left: 0.4rem;
        font-size: 0.24rem;
        color: #939393;
        margin-bottom: 0.42rem;
        background: url(../img/icon_checked.png) no-repeat left top;
        background-size: 0.32rem 0.29rem;
        &.checked{
            background: url(../img/icon_uncheck.png) no-repeat left top;
            background-size: 0.29rem 0.29rem;
        }
        input{
            width: 0.5rem;
            height:0.3rem;
        }
        a{
            font-size: 0.24rem;
            color: #4c91f8;
        }
    }
}

/*登陆--login*/
.login{
    .forget{
        padding: 0 0.42rem;
        margin-top: 0.25rem;
        overflow: hidden;
        a{
            font-size: 0.24rem;
            color: @main_color;
            float: right;
            &:nth-of-type(1){
                color:#4c91f8;
                float: left;
            };
        }
    }
    .btn_box{
        clear: both;
        padding: 0 0.5rem;
        margin-top: 0.45rem;
        overflow: hidden;
        a{
            display: block;
            width: 2.84rem;
            height: 0.78rem;
            border-radius: 0.08rem;
            font-size: 0.32rem;
            color: #fff;
            box-sizing: border-box;
            background: @main_color;
            line-height: 0.78rem;
            text-align: center;
            float: right;
            &:nth-of-type(1){
                border:@main_color solid 1px;
                background: #fff;
                color: @main_color;
                float: left;
            };
        }

    }

}

/*忘记密码--forget password*/
.forget{
    .button{
        margin-top: 0.65rem;
    }
}

/*重置密码--reset-password*/

.reset_password{
    button{
        margin-top: 0.7rem;
    }
}

/*搜索页--search-page*/

.search{
    .search_box{
        width: 100%;
        height: 0.9rem;
        background: url(../img/icon_search2.png) no-repeat center;
        background-size: 100%;
        overflow: hidden;
        .border_1px(bottom);
        input{
            display: block;
            width: 6.1rem;
            float: right;
            line-height: 0.99rem;
            font-size: 0.26rem;
            background: transparent;
            margin-right: 0.2rem;
        }
    }
    .search_recommend{
        margin-top: 0.2rem;
        background:#fff;
        font-size: 0;
        .recent{
            padding-top: 0.26rem;
            span{
                margin-top: 0.25rem;
                margin-right: 0.3rem;
                margin-left: 0.1rem;
                display: inline-block;
                line-height: 0.56rem;
                padding: 0 0.22rem;
                background-color:#f5f5f5;
                color: @font_color2;
                font-size: 0.24rem;
                border-radius: 0.04rem;
                text-overflow: ellipsis;
                white-space: nowrap;
                overflow: hidden;
                max-width: 6rem;
            }
        }
        .hot{
            margin-top: 0.3rem;
            ul{
                margin-top: 0.18rem;
            }
            li{
                .border_1px(top);
                line-height: 0.75rem;
                font-size: 0.24rem;
                color: @font_color2;
                text-indent: 0.15rem;
            }
        }
    }
}


/*搜索结果--search result*/
.about_recommend{
    width: 100%;
    background: #fff;
    margin-top: 0.2rem;
    .faq{
        padding-top: 0.25rem;
        .nodata{
            padding-top: 0.3rem;
            padding-bottom: 0.56rem;
            img{
                width: 2.87rem;
                text-align: center;
                display: block;
                margin: 0 auto;
            }
        }
        .list_faq{
            ul{
                margin-top: 0.12rem;
            }
            li{
                .border_1px(bottom);
                line-height: 0.79rem;
                font-size: 0.24rem;
                color: @font_color2;
                text-indent: 0.15rem;
                &:nth-last-of-type(1):after{
                    display:none;
                };
            }
        }
        a.fast_order{
            display: block;
            width: 100%;
            height: 0.68rem;
            line-height: 0.68rem;
            text-align: center;
            font-size: 0.26rem;
            color: @font_color;
            background: #f9f9f9;
        }
    }
    .about_serve{
        border-top: #f2f2f2 solid 0.2rem;
        padding-top: 0.25rem;
        padding-bottom: 0.9rem;
        .nodata{
            padding-bottom: 1.5rem;
            padding-top: 1.3rem;
            img{
                width: 1.55rem;
                display: block;
                margin:0 auto;
            }
        }
        .item_list{
            ul{
                margin-top: 0.15rem;
            }
            li{
                .border_1px(bottom);
                .item_img,.item_des{
                    margin-top: 0.25rem;
                    margin-bottom: 0.25rem;
                }
                .item_img{
                    margin-left: 0.33rem;
                }
            }
        }
    }

}

/*SKU列表*/
.sort{
    font-size: 0;
    .sort_list{
        width: 1.79rem;
        max-height: 100%;
        overflow-y:scroll;
        background-color: #fff;
        position: fixed;
        top: 0;
        display: inline-block;
        vertical-align: top;
        &:after{
            content:"";
            display: block;
            height: 100%;
            width: 1px;
            position: absolute;
            right: 0;
            top: 0;
            background-color: #e8e8e8;
        };
        li{
            font-size: 0.3rem;
            color: @font_color;
            line-height: 0.85rem;
            position: relative;
            text-align: center;
            &.on{
                color: @main_color;
            };
            &.on:after{
                content:"";
                display: block;
                width: 0.06rem;
                height: 0.5rem;
                background: @main_color;
                position: absolute;
                left: 0;
                top: 0.17rem;
            };
        }
    }
    .fix_list{
        width: 5.4rem;
        margin-left: 1.8rem;
        display: inline-block;
        vertical-align: top;
        background-color: #fff;
        ul{
            width: 4.96rem;
            margin:0 auto;
            display: none;
            &.on{
                display: block;
            }
            li{
                font-size: 0;
                padding: 0.35rem 0;
                .border_1px(bottom);
                &:nth-last-of-type(1):after{
                    display:none;
                };
                a{
                    display: block;
                }
                .fix_pic{
                    width: 1.31rem;
                    height: 1.31rem;
                    display: inline-block;
                    vertical-align: top;
                    overflow: hidden;
                    margin-right: 0.33rem;
                    margin-left: 0.2rem;
                    img{
                        width: 100%;
                    }
                }
                .fix_des{
                    width: 3.08rem;
                    display: inline-block;
                    vertical-align: top;
                    position: relative;
                    height: 1.35rem;
                    h2{
                        font-size: 0.26rem;
                        color: @font_color;
                        .ellipsis
                    }
                    p{
                        font-size: 0.22rem;
                        color: @font_color3;
                        margin: 0.04rem 0;
                        overflow : hidden;
                        text-overflow: ellipsis;
                        display: -webkit-box;
                        display: -moz-box;
                        -webkit-line-clamp:2;
                        -webkit-box-orient: vertical;
                        line-height: 0.28rem;
                    }
                    span{
                        font-size: 0.26rem;
                        color: @main_color;
                        display: block;
                        position: absolute;
                        bottom: -0.03rem;
                    }

                }
            }
        }
    }
}
@media (-webkit-min-device-pixel-ratio: 1.5) {
        .sort_list:after{
            transform: scaleX(0.7)
        }
    }
@media (-webkit-min-device-pixel-ratio: 2) {
        .sort_list:after{
            transform: scaleX(0.5)
        }
    }

/*快速下单--order_fastly*/
.order_fast{
    .order_fast_link{
        .border_1px(bottom);
        background: #fff;
        .wrap{
            .common_arrow();
            a{
                font-size: 0.26rem;
                color: @font_color;
                display: block;
            }
        }
    }
    .order_fast_form{
        width: 100%;
        margin-top: 0.2rem;
        form{
            .form_con{
                background: #fff;
            }
            .textarea{
                height: 1.06rem;
                padding-top: 0.18rem;
                position: relative;
                textarea{
                    resize: none;
                    width: 100%;
                    font-size: 0.24rem;
                    height: 1.06rem;
                 }
                p{
                    position: absolute;
                    right: 0;
                    bottom: 0;
                    font-size: 0.2rem;
                    color: @font_color3;
                    display: inline;
                }
            }
        }
    }

}

/*.维修详情--fix_details*/
.fix_details{
    border-bottom:#fff solid 1rem;
}
.fix_img{
    width: 100%;
    height: auto;
}
.fix_item_info{
    background: #fff;
    padding-top: 0.2rem;
    .limiting{
        h1{
            font-size: 0.3rem;
            color: @font_color;
            text-indent: 0.02rem;
        }
        p{
            font-size: 0.36rem;
            color: @main_color;
            margin-top: 0.1rem;
            s{
                color: @font_color3;
                font-size: 0.26rem;
                margin-left: 0.05rem;
            }
            span{
                display: inline-block;
                margin-left: 0.2rem;
                width: 1.19rem;
                height: 0.34rem;
                background: url(../img/icon_limit.png) no-repeat;
                background-size: 100%;
            }
        }
    }
    .unlimit{
        padding-bottom: 0.1rem;
        line-height: 0.45rem;
        h1{
            font-size: 0.3rem;
            color: @font_color;
            display: inline-block;
            vertical-align: top;
            line-height: 0.45rem;
            text-indent: 0.02rem;
        }
        p{
            color: @main_color;
            line-height: 0.45rem;
            font-size: 0.22rem;
            display: inline-block;
            vertical-align: top;
            margin-left: 0.45rem;
            span{
                font-size: 0.36rem;
            }
        }
    }
    .select_specific{
        .common_arrow();
        font-size: 0.26rem;
        color: @font_color3;
        .border_1px(top);
        margin-top: 0.1rem;
        text-indent: 0.02rem;
    }
}
.fix_wrap{
    width: 100%;
    margin-top: 0.2rem;

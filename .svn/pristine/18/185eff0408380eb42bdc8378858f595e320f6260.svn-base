    .fill_bill_3{
        padding: 0.3rem 0.25rem;
        h1{
            font-size: 0.3rem;
            color: @font_color;
            margin-bottom: 0.1rem;
        }
        ul{
            margin-top: 0.15rem;
        }
        li{
            font-size: 0.24rem;
            color: @font_color2;
            line-height: 0.4rem;
        }
    }
}


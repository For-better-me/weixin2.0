/*commom-style*/

@main_color:#fb870d;
@font_color:#333;
@font_color2:#666;
@font_color3:#999;
@blue_color:#4c91f8;
.font{
    font-size: 0.28rem;
    color: @font_color;
    text-indent: 0.11rem;
}
.font2{
    font-size: 0.28rem;
    color: @font_color !important;
}
.font3{
    font-size: 0.26rem;
    color: @font_color !important;
}
.main_color{
    color: @main_color !important;
}
.price{
    color: @main_color !important;
    font-size: 0.28rem;
}
.top15{
    margin-top: 0.15rem;
}
.none{
    display: none;
}
/*...省略号*/
.ellipsis{
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.common_arrow(@width:0.9rem){
    height: @width;
    background: url(../img/icon_arrow.png) no-repeat right center;
    background-size: 0.12rem 0.21rem;
    line-height: @width;
}
.border_1px(bottom){
    position: relative;
    &:after{
        content: "";
        position: absolute;
        bottom: 0;
        left: 0;
        width: 100%;
        border-top: 1px solid #e8e8e8;
    };
    @media (-webkit-min-device-pixel-ratio: 1.5) {
        &:after{
            transform: scaleY(0.7)
        }
    }

    @media (-webkit-min-device-pixel-ratio: 2) {
        &:after{
            transform: scaleY(0.5)
        }
    }
}
.border_1px(top){
    position: relative;
    &:before{
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        border-top: 1px solid #e8e8e8;
    };
    @media (-webkit-min-device-pixel-ratio: 1.5) {
        &:before{
            transform: scaleY(0.7)
        }
    }

    @media (-webkit-min-device-pixel-ratio: 2) {
        &:before{
            transform: scaleY(0.5)
        }
    }
}
.border_1px(right,@height,@top){
    position: relative;
    &:before{
        content: "";
        position: absolute;
        top: @top;
        right: 0;
        height: @height;
        border-right: 1px solid #e8e8e8;
    };
    @media (-webkit-min-device-pixel-ratio: 1.5) {
        &:before{
            transform: scaleX(0.7)
        }
    }

    @media (-webkit-min-device-pixel-ratio: 2) {
        &:before{
            transform: scaleX(0.5)
        }
    }
}

.button{
    width: 6.2rem;
    height: 0.88rem;
    border-radius: 0.09rem;
    margin:0 auto;
    font-size: 0.32rem;
    color: #fff;
    background-color: @main_color;
    display: block;
}

.order_btn{
    .border_1px(top);
    position: fixed;
    bottom: 0;
    z-index: 99;
    width: 100%;
    height: 1.18rem;
    background-color: #fff;
    button{
        width: 6.6rem;
        height: 0.88rem;
        background-color: @main_color;
        color: #fff;
        font-size: 0.32rem;
        margin:0.15rem auto;
        display: block;
        border-radius: 0.04rem;
    }
}

.form_btn{
    width: 100%;
    height: 0.88rem;
    display: block;
    line-height: 0.88rem;
    text-align: center;
    font-size: 0.32rem;
    color: @main_color;
    background: #fff;
    margin-top: 0.3rem;
}
.form_btn2{
    width: 100%;
    height: 0.88rem;
    display: block;
    line-height: 0.88rem;
    text-align: center;
    font-size: 0.32rem;
    background: @main_color;
    color: #fff;
    position: fixed;
    bottom: 0;
}
/*register,login*/
.form_common{
    width: 100%;
    background: #fff;
    .border_1px(top);
    .border_1px(bottom);
    margin-top: 0.2rem;
    li{
        height: 1rem;
        &:nth-last-of-type(1) p:after{
            display:none;
        };
        img{
            width: 0.4rem;
            height: 0.4rem;
            display: inline-block;
            vertical-align:middle;
            padding-left: 0.35rem;
            padding-right: 0.17rem;
        }
        p{
            display: inline-block;
            line-height: 0.99rem;
            width: 6.2rem;
            .border_1px(bottom);
            input{
                width: 4.5rem;
                height: 0.9rem;
            }
            span{
                font-size: 0.24rem;
                color: @main_color;
            }

        }
        .icon_password{
            width: 0.6rem;
            height: 0.99rem;
            float: right;
            background: url(../img/icon_close.png) no-repeat center;
            vertical-align: middle;
            background-size: 0.4rem;
            margin-right: 0.42rem;
            &.open{
                background: url(../img/icon_open.png) no-repeat center;
                background-size: 0.4rem;

            }
        }
    }
}
.flex{
    display: -webkit-flex;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.flex_item{
    flex-grow: 1;
}
.tit_common{
    height: 0.94rem;
    line-height: 0.94rem;
    background: #fff;
    img{
        width: 0.33rem;
        height: 0.33rem;
        margin: 0.3rem;
        margin-right: 0.25rem;
        margin-top: 0.26rem;
    }
    p{
        font-size: 0.28rem;
        display: inline-block;
        vertical-align: top;
    }
}

.item_list{
    li{
        font-size: 0;
        a{
            display: block;
        }
    }
    .item_img{
        width: 1.84rem;
        height: 1.84rem;
        overflow: hidden;
        position: relative;
        display: inline-block;
        vertical-align: top;
        margin: 0.5rem 0.56rem;
        margin-right: 0.96rem;
        img{
            position: absolute;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            margin:auto;
            max-width: 100%;
        }
    }
    .item_des{
        width: 3.22rem;
        margin: 0.5rem 0;
        display: inline-block;
        vertical-align: top;
        font-size: 0.26rem;
        h2{
            color: @font_color;
            margin-top: 0.3rem;
            text-overflow: ellipsis;
            white-space: nowrap;
            overflow: hidden;
            font-size: 0.26rem;
        }
        p{
            font-size: 0.24rem;
            color: @font_color3;
            margin: 0.18rem 0;
            text-overflow: ellipsis;
            white-space: nowrap;
            overflow: hidden;
        }
        span{
            color: @main_color;
            font-size: 0.26rem;
            display: block;
        }
    }
}
.footer_menu{
    .border_1px(top);
    position: fixed;
    bottom: 0;
    z-index: 99;
    font-size: 0;
    padding-top: 0.12rem;
    padding-bottom: 0.1rem;
    background: #fff;
    width: 100%;
    dl{
        width: 33.33%;
        display: inline-block;
        vertical-align: top;
        &.on dd{
            color: @main_color;
        }
        &:nth-of-type(1) dt{
            background:url(../img/icon_index.png) no-repeat center;
            background-size: 100%;
        };
        &:nth-of-type(2) dt{
            background:url(../img/icon_mail.png) no-repeat center;
            background-size: 100%;
        };
        &:nth-of-type(3) dt{
            background:url(../img/icon_my.png) no-repeat center;
            background-size: 100%;
        };
        &:nth-of-type(1).on dt{
            background:url(../img/icon_index1.png) no-repeat center;
            background-size: 100%;
        };
        &:nth-of-type(2).on dt{
            background:url(../img/icon_mail1.png) no-repeat center;
            background-size: 100%;
        };
        &:nth-of-type(3).on dt{
            background:url(../img/icon_my1.png) no-repeat center;
            background-size: 100%;
        };
        a{
            display: block;
        }
        dt{
            width: 0.4rem;
            height: 0.4rem;
            margin:0 auto;
           
        }
        dd{
            text-align: center;
            margin-top: 0.08rem;
            color: @font_color2;
        }
    }
}
/*index 首页 --style*/

.top{
    width: 100%;
    position: absolute;
    top: 0.2rem;
    z-index: 9;
}
.top .top_addr{
    display: inline-block;
    vertical-align: top;
    height: 0.5rem;
    width: auto;
    background: url(../img/icon_addr.png) no-repeat center;
    background-size: 100% 100%;
    padding-left: 0.5rem;
    padding-right: 0.2rem;
    margin-left: 0.2rem;
}
.top .top_addr>a{
    display: block;
}
.top .top_addr>a>span{
    font-size: 0.24rem;
    color: #fff;
    line-height: 0.51rem;
}
.top .top_search{
    width: 3.42rem;
    height: 0.5rem;
    position: relative;
    background: url(../img/icon_search.png) no-repeat;
    display: inline-block;
    vertical-align: top;
    background-size: 100%;
    margin-left: 0.2rem;
}
.top .top_search>ul{}
.top .top_search>ul>li{
    font-size: 0.24rem;
    color: #999;
    line-height: 0.52rem;
    text-indent: 0.56rem;
}
.banner{
    position: relative;
    width: 100%;
    height: 3.57rem;
    .bd ul>li{
       width: 100%;
       height: 3.57rem;
       overflow: hidden;
       a{
        display: block;
       } 
    }
    .hd{
        position: absolute;
        bottom: 0rem;
        z-index: 9;
        left: 0;
        right: 0;
        ul{
            text-align: center;
            li{
                font-size: 0;
                width: 0.08rem;
                height: 0.08rem;
                border-radius: 50%;
                display: inline-block;
                vertical-align: top;
                background: #ccc;
                margin:0 0.08rem;
                box-shadow: 0 0 1px 1px #eee;
                &.on{
                    background: #222;
                }
            }
        }
    }
}

.class_one{
    font-size: 0;
    padding:0.35rem 0.1rem;
    padding-bottom: 0.12rem;
    background:#fff;
    margin-bottom: 0.2rem;
    a{
        display: inline-block;
        width: 20%;
        img{
            width: 0.85rem;
            height: 0.85rem;
            display: block;
            margin:0 auto;
        }
        p{
            font-size: 0.24rem;
            text-align: center;
            margin-top: 0.15rem;
            margin-bottom: 0.22rem;
        }

    }

}
.selection_item{
    width: 100%;
    background: #fff;
    .more{
        width: 100%;
        line-height: 0.75rem;
        font-size: 0.26rem;
        text-align: center;
        color: #f9ac5b;
        .border_1px(bottom);
        display: block;
    }
    ul{
        overflow: hidden;
    }
    li{
        float: left;
        overflow: hidden;
        position: relative;
        &:nth-of-type(1){

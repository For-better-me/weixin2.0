/*订单支付页面验证弹框*/
.layer_pay{
    width: 5.6rem;
    height: 2.57rem;
    padding: 0.3rem;
    border-radius: 0.1rem;
    position: fixed;
    left: 0;
    right: 0;
    top: 30%;
    /*bottom: 0;*/
    margin:auto;
    display: none;
    background: #fff;
    z-index: 111;
    h1{
        font-size: 0.28rem;
        color: @font_color;
        margin-bottom: 0.35rem;
    }
    span,p{
        display: block;
        padding-left: 0.6rem;
        color: @font_color3;
        font-size: 0.26rem;
        margin-top: 0.25rem;
    }
    p{
        b{
            color: @main_color;
            font-size: 0.26rem;
            display: inline-block;
            vertical-align: top;
            margin-left: 0.2rem;
        }
        input{
            width:0.9rem;
            border-bottom: #333 solid 1px;
            text-align: center;
            font-size: 0.3rem;
            color: @font_color;
        }
    }
    button{
        font-size: 0.28rem;
        padding:0.3rem;
        position: absolute;
        bottom: 0;
        right: 0;
        background: transparent;
    }
}

/*城市列表*/
.city_wrap{
    width: 100%;
    background: #fff;
    padding-top: 0.2rem;
}
.box{
    padding-bottom: 0.1rem;
    .city{
        overflow-y:scroll;
    }
}
.search_city{
    .wrap{
        line-height: 0.75rem;
        .border_1px(bottom);
        background: url(../img/icon_search3.png) no-repeat left center;
        background-position-x: 0.1rem;
        background-size: 0.32rem 0.31rem;
        text-indent: 0.63rem;
        input{
            width: 90%;
            line-height: 0.75rem;
            font-size: 0.26rem;
        }
    }
}
.city-location{
    line-height: 0.88rem;
    p{
        font-size: 0.26rem;
        color: @main_color;
        text-indent: 0.66rem;
        background: url(../img/icon_addr5.png) no-repeat left center;
        background-size: 0.28rem 0.37rem;
        background-position-x: 0.1rem;
        span{
            margin-left: 0.12rem;
            font-size: 0.26rem;
        }
        a{
            float: right;
            font-size: 0.26rem;
            color: @main_color;
            margin-right: 0.1rem;

        }
    }
}
.letter{
    width: 0.6rem;
    position: fixed;
    top:1.82rem;
    right: 0;
    text-align: center;
    z-index: 99;
    padding: 0.05rem 0;
    a{
        width: 100%;
        text-align: center;
        line-height: 0.38rem;
        font-size: 0.24rem;
        color: @main_color;
        display: inline-block;
    }
}
.city-letter{
    width: 100%;
    height: 0.4rem;
    display: block;
    background:#f5f5f5;
    line-height: 0.4rem;
    text-indent: 0.3rem;
    font-size: 0.22rem;
}
.city-list{
    p{
        height: 0.9rem;
        line-height: 0.9rem;
        .border_1px(bottom);
        font-size: 0.26rem;
        color: @font_color;
        text-indent: 0.3rem;
    }
}
.city-hot{
    padding-bottom: 0.3rem;
    ul{
        padding: 0 0.7rem;
        font-size: 0;
        li{
            width: 1.4rem;
            height: 0.5rem;
            border:#e2e2e2 solid 1px;
            line-height: 0.5rem;
            text-align: center;
            font-size: 0.26rem;
            display: inline-block;
            background: #f5f5f5;
            margin-top: 0.3rem;
            margin-right: 0.7rem;
            border-radius: 0.02rem;
            &:nth-of-type(3){
                margin-right: 0;
            };
        }

    }
}
.showLetter{
        position: fixed;
        color: #ff943e;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        margin: auto;
        text-align: center;
        /*display: none;*/
        z-index: 999;
        opacity: 0;
        width: 1rem;
        height: 1rem;
        span{
                display: block;
                width: 1rem;
                height: 1rem;
                line-height: 1rem;
                font-size: .6rem;
                border: #ff943e 1px solid;
                border-radius: 50%;
        }
}

/*问题详情*/


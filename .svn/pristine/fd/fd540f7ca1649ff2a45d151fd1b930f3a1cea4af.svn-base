}
.about_recommend .about_serve .item_list li .item_img {
  margin-left: 0.33rem;
}
/*SKU列表*/
.sort {
  font-size: 0;
}
.sort .sort_list {
  width: 1.79rem;
  max-height: 100%;
  overflow-y: scroll;
  background-color: #fff;
  position: fixed;
  top: 0;
  display: inline-block;
  vertical-align: top;
}
.sort .sort_list:after {
  content: "";
  display: block;
  height: 100%;
  width: 1px;
  position: absolute;
  right: 0;
  top: 0;
  background-color: #e8e8e8;
}
.sort .sort_list li {
  font-size: 0.3rem;
  color: #333333;
  line-height: 0.85rem;
  position: relative;
  text-align: center;
}
.sort .sort_list li.on {
  color: #fb870d;
}
.sort .sort_list li.on:after {
  content: "";
  display: block;
  width: 0.06rem;
  height: 0.5rem;
  background: #fb870d;
  position: absolute;
  left: 0;
  top: 0.17rem;
}
.sort .fix_list {
  width: 5.4rem;
  margin-left: 1.8rem;
  display: inline-block;
  vertical-align: top;
  background-color: #fff;
}
.sort .fix_list ul {
  width: 4.96rem;
  margin: 0 auto;
  display: none;
}
.sort .fix_list ul.on {
  display: block;
}
.sort .fix_list ul li {
  font-size: 0;
  padding: 0.35rem 0;
  position: relative;
}
.sort .fix_list ul li:after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  border-top: 1px solid #e8e8e8;
}
@media (-webkit-min-device-pixel-ratio: 1.5) {
  .sort .fix_list ul li:after {
    transform: scaleY(0.7);
  }
}
@media (-webkit-min-device-pixel-ratio: 2) {
  .sort .fix_list ul li:after {
    transform: scaleY(0.5);
  }
}
.sort .fix_list ul li a {
  display: block;
}
.sort .fix_list ul li .fix_pic {
  width: 1.31rem;
  height: 1.31rem;
  display: inline-block;
  vertical-align: top;
  overflow: hidden;
  margin-right: 0.33rem;
  margin-left: 0.2rem;
}
.sort .fix_list ul li .fix_pic img {
  width: 100%;

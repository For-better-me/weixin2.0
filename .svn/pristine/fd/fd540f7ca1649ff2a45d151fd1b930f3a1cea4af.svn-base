  line-height: 0.88rem;
  float: right;
  margin-right: 0.28rem;
  font-size: 0.26rem;
  color: #666666;
}
.set .set_list ul li a.no_bg {
  background: none;
}
.set button {
  margin-top: 0.2rem;
}
